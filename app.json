{
  "expo": {
    "name": "Zapweb.app",
    "slug": "zapweb-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zapweb-logo.svg",
    "scheme": "zapweb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/zapweb-logo.svg",
        "backgroundColor": "#4F46E5"
      },
      "package": "com.zapweb.notesapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/zapweb-logo.svg",
      "name": "Zapweb.app - Smart Note Taking",
      "shortName": "Zapweb",
      "description": "A powerful note-taking app with AI features"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/zapweb-logo.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4F46E5"
        }
      ]
    ],
    "scheme": "myapp",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supabaseUrl": "https://ilkyxnibclxsgpklhteq.supabase.co",
